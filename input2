var a float32 = 1.7
var b float32 = 1.1
var c int = 5
var d int = 12
var x int

x = 1.7 % 1.1 	// c_flt % c_flt

x = a % 1.1 	// flt % c_flt

x = 1.1 % a 	// c_flt % flt

x = 5 % 1.7 	// c_int % c_flt

x = c % a 		// int % flt

x = a % c 		// flt % int

x = 5 % b		// c_int % flt

x = b % 5		// flt % c_int

x = 12 % 7		// i_cnt % i_cnt

x = c % 7		// i_val % i_cnt

x = 7 % c + 7	// i_cnt % i_val
